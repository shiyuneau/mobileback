    <update id="applyExecute" parameterType="map">
        update Scholarshipapplication set Status = #{status} where GUID =#{applyrecordid} and StudentGUID = #{userid}
    </update>
    <select id="applyList" resultType="com.sy.mobileback.accessdb.domain.ScholarshipapplicationEntity">
        select * from Scholarshipapplication where StudentGUID = #{userId}
    </select>
    <select id="applySuccessList" resultType="com.sy.mobileback.accessdb.domain.ScholarshipapplicationEntity">
        select   apply.* from  scholarshipapplication apply
        left JOIN school sch on sch.GUID = apply.recominstitutions
        left JOIN manager man on sch.ManagerGUID =man.GUID where man.guid = #{managerGUID} and apply.status = #{status}
    </select>
